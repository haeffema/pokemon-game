{
  "name": "pokemon-game",
  "version": "0.2.0",
  "description": "Discord bot that simulates a simplified version of a pokemon game.",
  "type": "module",
  "scripts": {
    "pre-commit": "lint-staged",
    "format": "prettier --write \"**/*.js\" \"**/*.json\"",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix ",
    "bot": "node src/main.js",
    "sync": "node src/deploy-commands.js",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Jan and Max",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@smogon/calc": "^0.10.0",
    "canvas": "^3.1.0",
    "child_process": "^1.0.2",
    "cron": "^4.3.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "events": "^3.3.0",
    "jsdom": "^26.1.0",
    "mysql": "^2.18.1",
    "node-cron": "^4.0.7",
    "path": "^0.12.7",
    "pokemon-showdown": "^0.11.10",
    "prompt-sync": "^4.2.0",
    "quickchart-js": "^3.1.3",
    "util": "^0.12.5"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "eslint . "
    ]
  }
